# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/docroot/*

# Ignore osx file fingerprints
.DS_Store

# Ignore gitlab access token commits
auth.json
.idea

# Ignore git files
.editorconfig
.gitattributes

# Include specific files and folders
# Don't ignore sites folder
!/docroot/sites
# But ignore everything inside sites folder
/docroot/sites/*
# Don't ignore the environents folder inside that tho
!/docroot/sites/environments
# Don't ignore the default folder inside that tho
!docroot/sites/default
# But do ignore everything inside the actual folder
docroot/sites/default/**
# Except this subfolder
!/docroot/sites/default/files
# But do ignore everything inside of the subfolder
/docroot/sites/default/files/*
# Except this one file
!/docroot/sites/default/files/.gitkeep
# Don't ignore the styleguide folder
!/docroot/styleguide

config/**
!config/sync
config/sync/**
!config/sync/.gitkeep

!config/splits
config/splits/**

!config/splits/dev/
config/splits/dev/**
!config/splits/dev/.gitkeep

!config/splits/staging/
config/splits/staging/**
!config/splits/staging/.gitkeep

!config/splits/local/
config/splits/local/**
!config/splits/local/.gitkeep

!config/splits/live/
config/splits/live/**
!config/splits/live/.gitkeep

!config/splits/whitelist/
config/splits/whitelist/**
!config/splits/whitelist/.gitkeep

private/files/**
!private/files/.gitkeep

# Ignore settings_local, is generated by infra
/etc/drupal/settings_local.php
/docker-compose.*.yml

# Launchpad .gitignore begin DO NOT REMOVE OR EDIT THIS LINE
/.docker-data
/.docker-sync
/.env
/docker-compose.yml
/docker-compose.override.yml
/docker-sync.yml
/docroot/sites/default/*.local.php
/docroot/sites/default/*.aliases.drushrc.php
*.unison.tmp
# Launchpad .gitignore end DO NOT REMOVE OR EDIT THIS LINE
.dlp.yml
